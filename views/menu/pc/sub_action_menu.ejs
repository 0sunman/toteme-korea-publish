<div class="site-header__misc" js-site-header="actions">
        <!-- 계정관리 -->
        <div class="site-header__col site-header__col--desktop">
            <button class="accountButton site-header__icon site-header__icon--account" data-right-header-button-type="accountNav">
                <svg class="icon icon--medium icon__account" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path fill-rule="evenodd" clip-rule="evenodd" d="M11 4.5a3 3 0 11-6 0 3 3 0 016 0zm1 0a4 4 0 11-8 0 4 4 0 018 0zm-10 7a1 1 0 011-1h10a1 1 0 011 1v3H2v-3zm-1 0a2 2 0 012-2h10a2 2 0 012 2v4H1v-4z"></path>
                </svg>
                <span class="visually-hidden">
                Account
                </span>
            </button>
            <div id="accountNav" class="account-drawer drawer critical-hide">
                <div class="account-drawer__content drawer__content">
                <div class="account-drawer__header">
                    <div class="account-drawer__title h3">
                        <span class="account-drawer__title-text">Account</span>
                        <button class="account-drawer__close button">
                            <span class="visually-hidden">Close</span>
                            <svg class="icon icon--large icon__close">
                            <path fill-rule="evenodd" clip-rule="evenodd" d="M23 12c0-6.075-4.925-11-11-11S1 5.925 1 12s4.925 11 11 11 11-4.925 11-11zM2 12C2 6.477 6.477 2 12 2s10 4.477 10 10-4.477 10-10 10S2 17.523 2 12zm6.593-2.7l.707-.707 2.7 2.7 2.7-2.7.707.707-2.7 2.7 2.7 2.7-.707.707-2.7-2.7-2.7 2.7-.707-.707 2.7-2.7-2.7-2.7z"></path>
                            </svg>
                        </button>
                    </div>
                </div>
                <div class="account-drawer__body">
                    <ul class="site-header__account-navlist navlist navlist--large critical-hide">
                        <li class="navlist__item">
                            <a class="navlist__link text-link" href="/account/login">
                            Sign in
                            </a>
                        </li>
                        <li class="navlist__item">
                            <a class="navlist__link text-link" href="/account/register">
                            Create account
                            </a>
                        </li>
                        <li class="navlist__item navlist__item--margin ">
                            <a class="navlist__link text-link" href="/account/login#reactivate">
                            Reactivate account
                            </a>
                        </li>
                        <li class="navlist__item">
                            <a class="navlist__link text-link" href="/account/login#recover">
                            Forgot password
                            </a>
                        </li>
                    </ul>
                </div>
                </div>
            </div>
            <button class="site-header__icon site-header__icon--search" data-right-header-button-type="searchBarHeader">
                <span class="icon-fallback-text">
                Search
                </span>
                <svg class="icon icon--medium icon__search" viewBox="0 0 16 16" fill="none" xmlns="http://www.w3.org/2000/svg">
                <g clip-path="url(#clip0)">
                    <path fill-rule="evenodd" clip-rule="evenodd" d="M10.598 10.131c1.655-1.9 1.553-4.811-.289-6.652-1.92-1.922-5.007-1.95-6.894-.064-1.886 1.887-1.858 4.973.064 6.895 1.757 1.757 4.49 1.93 6.387.503l3.34 3.34.706-.708-3.314-3.314zm-.921-.454c-1.509 1.509-3.978 1.486-5.515-.05-1.537-1.538-1.56-4.007-.05-5.516 1.508-1.509 3.977-1.486 5.514.05 1.537 1.538 1.56 4.007.051 5.516z"></path>
                </g>
                <defs>
                    <clipPath id="clip0">
                        <path fill="#fff" d="M0 0h16v16H0z"></path>
                    </clipPath>
                </defs>
                </svg>
            </button>
        </div>
        <div class="site-header__col">
            <div class="site-header__icon site-header__icon--wishlist">
                <!-- start: include 'wishlist-link' -->
                <a href="/account/login?wk-redirect=%7B%22path%22%3A%22%2Fpages%2Fwishlist%22%7D" class="wk-link wk-empty" title="View Wishlist">
