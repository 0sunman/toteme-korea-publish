    <script>
        function init(){
            $(".site-header__col--desktop .site-header__icon.site-header__icon--search").click();            
            return showOrhideArray({ // show
             array:[
                 ".suggestions__menu-bar.sorry", ".suggestions__menu-bar.only_one",  
                 ".suggestions__menu-bar.suggestions__menu-bar--two-items", 
                 ".suggestions__product-grid.card-grid.has-products"],
             show:false// hide             
            })
        }
        function doSuccessOne(){
            init()({
                array:[".suggestions__menu-bar.only_one",".suggestions__product-grid.card-grid.has-products"],
                show:true,
                end:true
            })
        }
        function doSuccessTwo(){
            init()({
                array:[".suggestions__menu-bar.suggestions__menu-bar--two-items",".suggestions__product-grid.card-grid.has-products"],
                show:true,
                end:true
            })
        }
        function doFail(){
            init()({
                array:[".suggestions__menu-bar.sorry"],
                show:true,
                end:true
            })
        }
    </script>

